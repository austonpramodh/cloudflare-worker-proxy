{
  "compilerOptions": {
    "lib": [
      "esnext",
      "WebWorker"
    ],
    "removeComments": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "dist",
    "esModuleInterop": true,
    "noEmit": true,
    "types": [
      "@cloudflare/workers-types",
    ]
  },
  "include": [
    "./src/*.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*",
    "dist/"
  ]
}